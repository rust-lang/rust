//@edition: 2021
#![warn(clippy::search_is_some)]

fn main() {
    fn ref_bindings() {
        let _ = ![&(&1, 2), &(&3, 4), &(&5, 4)].iter().any(|(&x, y)| x == *y);
        //~^ search_is_some
        let _ = ![&(&1, 2), &(&3, 4), &(&5, 4)].iter().any(|(&x, y)| x == *y);
        //~^ search_is_some
        let _ = ![&(&1, 2), &(&3, 4), &(&5, 4)]
            //~^ search_is_some
            .iter().any(|&&(&x, ref y)| x == *y);
    }
}
