// run-rustfix
#![warn(clippy::suboptimal_flops)]

fn main() {
    let x = 3f32;
    let y = 5f32;
    let _ = x.log(y);
    let _ = (x as f32).log(y);
    let _ = x.log(y);
    let _ = x.log(y);
    let _ = x.log(y);
    // Cases where the lint shouldn't be applied
    let _ = x.ln() / y.powf(3.2);
    let _ = x.powf(3.2) / y.powf(3.2);
    let _ = x.powf(3.2) / y.ln();
    let _ = x.log(5f32) / y.log(7f32);
}
