fn do_nothing<T>(_: T) {}

fn diverge<T>(_: T) -> ! {
    panic!()
}

fn plus_one(value: usize) -> usize {
    value + 1
}

struct HasResult {
    field: Result<usize, usize>,
}

impl HasResult {
    fn do_result_nothing(&self, value: usize) {}

    fn do_result_plus_one(&self, value: usize) -> usize {
        value + 1
    }
}

