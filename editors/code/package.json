                "defaults": {
                    "dark": "#4EC9B0",
                    "light": "#267F99",
                    "highContrast": "#4EC9B0"
                }
            },
            {
                "id": "ralsp.type.lifetime",
                "description": "Color for lifetimes parameters",
                "defaults": {
                    "dark": "#4EC9B0",
